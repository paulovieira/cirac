		select (gid + _offset) as gid, value, geom from geo.cirac_risk_cmbr_bx_res_inventory_b;

	-- append coimbra sul
	_offset := count(gid) from geo.cirac_risk_lx_res_inventory_b;
	raise notice 'coimbra baixa: %', _offset;
	insert into geo.cirac_risk_lx_res_inventory_b(gid, value, geom)
		select (gid + _offset) as gid, value, geom from geo.cirac_risk_cmbr_sul_res_inventory_b;

	-- append porto/gaia
	_offset := count(gid) from geo.cirac_risk_lx_res_inventory_b;
	raise notice 'coimbra sul: %', _offset;
	insert into geo.cirac_risk_lx_res_inventory_b(gid, value, geom)
		select (gid + _offset) as gid, value, geom from geo.cirac_risk_oporto_res_inventory_b;
		
END
